{
	"name": "Just Let Me Go",
	"version": "0",
	"manifest_version": 2,
	"author": "Lyubomyr Shaydariv",
	"description": "Removes tracking parameters and bypasses tracking redirections at some sites",
	"icons" :{
		"48" : "icon-48x48.png",
		"128" : "icon-128x128.png"
	},
	"content_security_policy": "object-src 'self'; script-src 'self';",
	"background": {
		"scripts": [
			"core.js",
			"modules/facebook.js",
			"modules/fbclid.js",
			"modules/ga.js",
			"modules/gclid.js",
			"modules/igshid.js",
			"modules/instagram.js",
			"modules/mailchimp.js",
			"modules/utm.js",
			"modules/evernote.js",
			"modules/google.js",
			"modules/mandrill.js",
			"modules/medium.js",
			"modules/reddit.js",
			"modules/tumblr.js",
			"modules/twitter.js",
			"modules/vk.js",
			"modules/yclid.js",
			"modules/youtube.js",
			"main.js"
		]
	},
	"permissions": [
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	]
}
